<Files ~ "\.(inc)$">
  order allow,deny
  deny from all
</Files>
Options +FollowSymlinks
RewriteEngine On
RewriteRule   ^cgi-bin/$ / [R]
RewriteRule   ^php/$  / [R]
RewriteRule   ^css/$  / [R]
RewriteRule   ^includes/$  / [R]
RewriteRule   ^images/$  / [R]
RewriteRule   ^js/$  / [R]
ErrorDocument 402 /404.html
ErrorDocument 403 /404.html 
ErrorDocument 404 /404.html
ErrorDocument 500 /404.html
ErrorDocument 501 /404.html